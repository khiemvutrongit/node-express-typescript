{
  "name": "product-manage",
  "version": "1.0.0",
  "description": "",
  "author": "KV",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "DEVELOP_ENV=dev node -r ts-node/register --inspect server.ts",
    "start:nodemon": "nodemon",
    "start:dist": "cd ../dist && npm run start && cd ../src",
    "test": "DEVELOP_ENV=dev jest",
    "test:watch": "DEVELOP_ENV=dev jest --watchAll",
    "test:dist": "cd ../dist && npm run test && cd ../src",
    "copyfile": "node copyfile",
    "clean": "cd .. && rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run copyfile && tsc && cd ../dist && npm i --no-fund && cd ../src"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "**/*"
    ],
    "exec": "npm run start",
    "ext": "ts"
  },
  "dependencies": {
    "async": "^3.2.2",
    "axios": "^0.24.0",
    "base-64": "^1.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "http-status": "^1.5.0",
    "jest": "^27.4.5",
    "js-base64": "^3.7.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.4",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/async": "^3.2.12",
    "@types/base-64": "^1.0.0",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/supertest": "^2.0.11",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.10",
    "webpack-node-externals": "^1.7.2"
  }
}
